display page

function display() {
    var a = document.myform.professors.value;
    var b = document.myform.assistant_professors.value;
    var c = document.myform.associate_professors.value;
    var d = document.myform.papers_published.value;
    var e = document.myform.third_results.value;
    var f = document.myform.fifth_results.value;
    var g = document.myform.seventh_results.value;
    var h = document.myform.third_intake.value;
    var i = document.myform.fifth_intake.value;
    var j = document.myform.seventh_intake.value;
    var k = document.myform.nMOU.value;
    var l = document.myform.MOU.value;
    var m = document.myform.nparticipation.value;
    var n = document.myform.participation.value;
    var o = document.myform.nrecognition.value;
    var p = document.myform.recognition.value;
    var q = document.myform.total_phd.value;
    var r = document.myform.registered.value;
    var s = document.myform.completion.value;
    var t = document.myform.thesis.value;
    var u = document.myform.fdp_industry.value;
    var v = document.myform.fdp_other.value;
    var w = document.myform.books.value;
    var x = document.myform.articles.value;
    var y = document.myform.collaborative_programs.value;
    var z = document.myform.tv_discussions.value;
    var a1 = document.myform.academic_council.value;
    var b1 = document.myform.member_bos.value;
    var c1 = document.myform.technical_committee.value;
    var d1 = document.myform.project_participation.value;
    var e1 = document.myform.certification_programs.value;
    var f1 = document.myform.ncourses.value;
    var g1 = document.myform.Patents.value;
    var h1 = document.myform.nconsultancy.value;
    var i1 = document.myform.input_students.value;
    var j1 = document.myform.training_details.value;
    var k1 = document.myform.competition_participated.value;
    var l1 = document.myform.competition_recognition.value;





    document.write("<h1>DAYANANDASAGAR COLLEGE OF ENGINEERING</h1>");

    document.write("<table><tr><td>Faculty strength in Department</td><td>Professors: " + a + "</td><td>Associate professors: " + b +"</td></tr></table>");
    document.write("Professors: "+a+"<br>");
    document.write("Associate professors: "+b + "<br>");
    document.write("Assistant professors: "+c + "<br>");
    document.write("value: "+d + "%" +"<br>");
    document.write("value: "+e +"%"+ "<br>");
    document.write("value: "+f + "%" +"<br>");
    document.write("value: "+g + "%" +"<br>");
    document.write("3rd semester "+h + "<br>");
    document.write("5th semester "+i + "<br>");
    document.write("7th semester "+j + "<br>");
    document.write("Number of MOU's "+k + "<br>");
    document.write(l + "<br>");
    document.write("Number of Participation "+m + "<br>");
    document.write(n + "<br>");
    document.write("Number of Recognition "+o + "<br>");
    document.write(p + "<br>");
    document.write("Total number of PHDs: "+q + "<br>");
    document.write("Registered: "+r + "<br>");
    document.write("Completion of VIVA: "+s + "<br>");
    document.write("Thesis Submission: "+t + "<br>");
    document.write("FDP's(industry): "+u + "<br>");
    document.write("FDP's(other): "+v + "<br>");
    document.write(w + "<br>");
    document.write(x + "<br>");
    document.write(y + "<br>");
    document.write(z + "<br>");
    document.write(a1 + "<br>");
    document.write(b1 + "<br>");
    document.write(c1 + "<br>");
    document.write(d1 + "<br>");
    document.write(e1 + "<br>");
    document.write(f1 + "<br>");
    document.write(g1 + "<br>");
    document.write(h1 + "<br>");
    document.write(i1 + "<br>");
    document.write(j1 + "<br>");
    document.write(k1 + "<br>");
    document.write(l1 + "<br>");
  
}
